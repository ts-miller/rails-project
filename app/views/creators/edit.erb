Edit a Creator<br>

<%= form_with model: @creator, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, autocomplete: "name", required: true %><br>
    <%= f.label :email %>
    <%= f.email_field :email, autocomplete: "email", required: true %><br>
    <%= f.submit %>
    <%= f.label :state %>
    <%= f.collection_select :state, all_states, :second, :second %><br>
<% end %>